<Window x:Class="WindowsCommucation.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DialogWindow" Height="300" Width="300">
    <StackPanel>
        <Button Name="true" Margin="10" Click="true_Click">true</Button>
        <Button Name="false" Margin="10" Click="false_Click">false</Button>
        <Button Name="null" Margin="10" Click="null_Click">null</Button>
    </StackPanel>
</Window>
