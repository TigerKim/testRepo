<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ResourceLibrary"
                    >
    <ImageBrush x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:CustomResources}, ResourceId=SadTileBrush}" 
                TileMode="Tile" ViewportUnits="Absolute"
                x:Shared="False"
                Viewport="0 0 32 32" ImageSource="ResourceLibrary;component/sadface.png" Opacity="0.3"
                />
    
</ResourceDictionary>